{
  "name": "blueprint_spa",
  "version": "1.0.0",
  "description": "An example webpack/react setup for medidata SPAs",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run karma-ci && npm run cucumber",
    "build": "webpack --color=false --config=./app_js/config/webpack.prod.config.js",
    "release": "true",
    "karma-watch": "karma start app_js/config/karma.dev.config.js",
    "karma-ci": "karma start app_js/config/karma.ci.config.js",
    "lint": "semistandard",
    "dev-server": "webpack-dev-server --config=./app_js/config/webpack.dev.config --hot --inline",
    "start:test": "NODE_ENV=test node app_js/server.js",
    "electron": "electron .",
    "dev": "concurrently \"npm start\" \"npm run dev-server\" \"npm run karma-watch\"",
    "en-translations": "node app_js/config/i18n/scripts/copy-en.js",
    "wdio": "wdio app_js/config/wdio.conf",
    "cucumber": "HTTP_PORT=4000 WEBPACK_PORT=4001 concurrently --kill-others --success 'first' \"npm run start:test\" \"npm run dev-server\" \"npm run wdio\"",
    "package-mac": "NODE_ENV=production npm run build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=Actions-document-edit-icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdsol/blueprint_spa.git"
  },
  "author": "Max Polun <maxpolun@gmail.com>",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/mdsol/blueprint_spa/issues"
  },
  "homepage": "https://github.com/mdsol/blueprint_spa#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.6",
    "ejs-electron": "^2.0.0",
    "electron": "^1.6.11",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "history": "^4.6.2",
    "image-webpack-loader": "^3.3.1",
    "lego": "https://s3.amazonaws.com/mdsol-resources/lego/lego-2.3.0.tgz",
    "lowdb": "^0.16.2",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0",
    "sandman-bower": "git+ssh://git@github.com/mdsol/sandman-bower.git#1.2.7",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "svg-sprite-loader": "^3.0.5",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "babel-plugin-react-intl": "^2.3.1",
    "concurrently": "^3.4.0",
    "electron-packager": "^8.7.2",
    "enzyme": "^2.8.2",
    "express-http-proxy": "^1.0.4",
    "jasmine-core": "^2.6.3",
    "jasmine-enzyme": "^3.3.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "react-addons-perf": "^15.4.2",
    "react-test-renderer": "^15.6.1",
    "semistandard": "^11.0.0",
    "wdio-cucumber-framework": "^0.3.1",
    "wdio-dot-reporter": "0.0.8",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-webpack-dev-server-service": "^1.0.0",
    "webdriverio": "^4.8.0",
    "webpack-dev-server": "^2.4.5"
  },
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "fit",
      "expect",
      "jasmine",
      "spyOn",
      "beforeEach",
      "afterEach",
      "__DEV__",
      "__TEST__",
      "__PRODUCTION__",
      "browser"
    ]
  }
}
